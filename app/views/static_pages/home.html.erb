<main>
  <header>
    <img src="" alt="logo">
    <nav>
      <ul>
        <% if current_user %>
          <li>
            <%= button_to(
              "Log Out",
              destroy_user_session_path,
              method: :delete
            ) %>
          </li>
        <% else %>
          <li>
            <%= link_to 'Login', new_user_session_url %>
          </li>
          <li>
            <%= link_to 'Sign Up', new_user_registration_url %>
          </li>
        <% end %>
      </ul>
    </nav>
  </header>

  <section>
    <%= image_tag 'hero-img' %>
  </section>

  <section>
    <h2>What's it all about</h2>
    <p></p>
  </section>
</main>


